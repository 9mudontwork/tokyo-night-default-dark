{
  "name": "Github Houston",
  "type": "dark",
  "include": "../../editor/houston.json",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": { "foreground": "#8e9cab" }
    },
    {
      "scope": ["constant.other.placeholder", "constant.character"],
      "settings": { "foreground": "#ff837a" }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language",
        "entity"
      ],
      "settings": { "foreground": "#81c8ff" }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": { "foreground": "#ffae5f" }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": { "foreground": "#edf5fc" }
    },
    {
      "scope": "entity.name.function",
      "settings": { "foreground": "#dab0ff" }
    },
    { "scope": ["entity.name.tag"], "settings": { "foreground": "#82f38c" } },
    {
      "scope": ["support.class.component"],
      "settings": { "foreground": "#82f38c" }
    },
    { "scope": "keyword", "settings": { "foreground": "#ff837a" } },
    {
      "scope": ["storage", "storage.type"],
      "settings": { "foreground": "#ff837a" }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": { "foreground": "#edf5fc" }
    },
    {
      "scope": ["string", "string punctuation.section.embedded source"],
      "settings": { "foreground": "#addeff" }
    },
    { "scope": "support", "settings": { "foreground": "#81c8ff" } },
    { "scope": "meta.property-name", "settings": { "foreground": "#81c8ff" } },
    { "scope": "variable", "settings": { "foreground": "#ffae5f" } },
    { "scope": "variable.other", "settings": { "foreground": "#edf5fc" } },
    {
      "scope": "invalid.broken",
      "settings": { "fontStyle": "italic", "foreground": "#ffa9a0" }
    },
    {
      "scope": "invalid.deprecated",
      "settings": { "fontStyle": "italic", "foreground": "#ffa9a0" }
    },
    {
      "scope": "invalid.illegal",
      "settings": { "fontStyle": "italic", "foreground": "#ffa9a0" }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": { "fontStyle": "italic", "foreground": "#ffa9a0" }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#ff837a",
        "foreground": "#f8feff",
        "content": "^M"
      }
    },
    { "scope": "message.error", "settings": { "foreground": "#ffa9a0" } },
    { "scope": "string variable", "settings": { "foreground": "#81c8ff" } },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": { "foreground": "#addeff" }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": { "foreground": "#addeff" }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": { "fontStyle": "bold", "foreground": "#82f38c" }
    },
    { "scope": "support.constant", "settings": { "foreground": "#81c8ff" } },
    { "scope": "support.variable", "settings": { "foreground": "#81c8ff" } },
    {
      "scope": "support.type.property-name.json",
      "settings": { "foreground": "#82f38c" }
    },
    {
      "scope": "meta.module-reference",
      "settings": { "foreground": "#81c8ff" }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": { "foreground": "#ffae5f" }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": { "fontStyle": "bold", "foreground": "#81c8ff" }
    },
    { "scope": "markup.quote", "settings": { "foreground": "#82f38c" } },
    {
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic", "foreground": "#edf5fc" }
    },
    {
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold", "foreground": "#edf5fc" }
    },
    { "scope": ["markup.underline"], "settings": { "fontStyle": "underline" } },
    {
      "scope": ["markup.strikethrough"],
      "settings": { "fontStyle": "strikethrough" }
    },
    { "scope": "markup.inline.raw", "settings": { "foreground": "#81c8ff" } },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": { "background": "#490202", "foreground": "#ffa9a0" }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": { "foreground": "#ff837a" }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": { "background": "#04260f", "foreground": "#82f38c" }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": { "background": "#5a1e02", "foreground": "#ffae5f" }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": { "foreground": "#1d232b", "background": "#81c8ff" }
    },
    {
      "scope": "meta.diff.range",
      "settings": { "foreground": "#dab0ff", "fontStyle": "bold" }
    },
    { "scope": "meta.diff.header", "settings": { "foreground": "#81c8ff" } },
    {
      "scope": "meta.separator",
      "settings": { "fontStyle": "bold", "foreground": "#81c8ff" }
    },
    { "scope": "meta.output", "settings": { "foreground": "#81c8ff" } },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": { "foreground": "#8e9cab" }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": { "foreground": "#ffa9a0" }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": { "foreground": "#addeff" }
    }
  ]
}
