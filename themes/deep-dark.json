{
  "name": "Tokyo i3 Deep Dark",
  "type": "dark",
  "colors": {
    "titleBar.activeBackground": "#191919cc",
    "editor.background": "#141414",
    "editor.foreground": "#D4D4D4",
    "editor.inactiveSelectionBackground": "#3A3D41",
    "editorIndentGuide.background1": "#404040",
    "editorIndentGuide.activeBackground1": "#707070",
    "editor.selectionHighlightBackground": "#ADD6FF26",
    "tab.lastPinnedBorder": "#ccc3",
    "sideBarSectionHeader.background": "#0000",
    "sideBarSectionHeader.border": "#ccc3",
    "sideBar.background": "#191919",
    "sideBarTitle.foreground": "#BBBBBB",
    "activityBar.background": "#1f1f1f",
    "list.dropBackground": "#383B3D",
    "activityBarBadge.background": "#007ACC",
    "input.placeholderForeground": "#A6A6A6",
    "menu.background": "#252526",
    "menu.foreground": "#CCCCCC",
    "statusBarItem.remoteForeground": "#FFF",
    "statusBarItem.remoteBackground": "#16825D",
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238",
    "ports.iconRunningProcessForeground": "#369432"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#7dcfff"
    },
    "variable.constant": {
      "foreground": "#e08f68"
    },
    "variable.defaultLibrary": {
      "foreground": "#dec76e"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "string.comment"],
      "settings": {
        "foreground": "#9ba0ac"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Embedded String Begin/End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.erb",
        "punctuation.section.embedded.end.erb",
        "source.elixir.embedded",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#accfd7"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#ff7682"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#ff7682"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#ff7682"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#ff7682"
      }
    },
    {
      "name": "Types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#6ca2fa"
      }
    },
    {
      "name": "Variable Object",
      "scope": [
        "variable.other.object",
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#6ca2fa"
      }
    },
    {
      "name": "Other variable",
      "scope": [
        "variable.other",
        "variable.other.property",
        "variable.other.block"
      ],
      "settings": {
        "foreground": "#accfd7"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#72c369"
      }
    },
    {
      "name": "Package Import",
      "scope": ["storage.modifier.import", "storage.modifier.package"],
      "settings": {
        "foreground": "#6ca2fa"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#72c369"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#accfd7"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": ["function.support.builtin", "function.support.core"],
      "settings": {
        "foreground": "#72c369"
      }
    },
    {
      "name": "Tag Name",
      "scope": ["entity.name.tag", "entity.name.tag.class.js"],
      "settings": {
        "foreground": "#ff7682"
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": ["entity.name.tag.class", "entity.name.tag.id"],
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#72c369"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.variable"],
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "Json Property",
      "scope": "support.dictionary.json",
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "StyleSheet Property name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#accfd7"
      }
    },
    {
      "name": "CSS: Pseudo Attribute Names",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.scss",
        "entity.other.attribute-name.pseudo-class.less",
        "entity.other.attribute-name.pseudo-class.sass",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.scss",
        "entity.other.attribute-name.pseudo-element.less",
        "entity.other.attribute-name.pseudo-element.sass"
      ],
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#72c369"
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": ["unit.css", "unit.scss", "unit.less", "unit.sass"],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "StyleSheet Function",
      "scope": [
        "function.css",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#c678dd",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#36c0c0",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON String",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "JSON String",
      "scope": ["constant.language.json"],
      "settings": {
        "foreground": "#d082e7"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#6ca2fa"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#a7a28d"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ff7682"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#72c369"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#36c0c0A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Punctuation",
      "scope": [
        "punctuation.definition.italic.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#9aa2b1"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#6ca2fa"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff7682"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#72c369"
      }
    },
    {
      "name": "Markup: Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#c678dd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Raw",
      "scope": ["markup.raw.inline.markdown", "markup.raw.block.markdown"],
      "settings": {
        "foreground": "#36c0c0"
      }
    },
    {
      "name": "Markup: List Punctuation",
      "scope": "punctuation.definition.list_item.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    }
  ]
}
