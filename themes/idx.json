      "settings": { "foreground": "#36c0c0" }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": { "fontStyle": "" }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": { "background": "#e889ff", "foreground": "#b0b0a8" }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": { "background": "#36c0c0", "foreground": "#b0b0a8" }
    },
    {
      "name": "JSON String",
      "scope": "support.type.property-name.json",
      "settings": { "foreground": "#36c0c0" }
    },
    {
      "name": "JSON String",
      "scope": ["constant.language.json"],
      "settings": { "foreground": "#DB8BF2" }
    },
    {
